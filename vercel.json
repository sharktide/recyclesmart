{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py", 
        "use": "@vercel/python"
      },
      {
        "src": "index.html", 
        "use": "@vercel/static"
      },
      {
        "src": "static/**", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/api/app.py"
      },
      {
        "src": "/(.*)", 
        "dest": "/index.html"
      }
    ]
  }
  